{
  "name": "react-typescript-webpack-prerender-template",
  "version": "1.0.0",
  "description": "A minimal React project template built by webpack with typescript and css modules support, also includes a prerender facility.",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist/*",
    "start": "ts-node webpack.ts serve",
    "build": "ts-node webpack.ts build && npm run prerender --",
    "build:dev": "ts-node webpack.ts build --mode=development && npm run prerender -- --api mock",
    "prerender": "node dist/prerender",
    "build:prerender": "ts-node webpack.ts build -e prerender",
    "build:prerender:dev": "ts-node webpack.ts build -e prerender -m development",
    "build:web": "ts-node webpack.ts build -e app",
    "build:web:dev": "ts-node webpack.ts build -e app -m development",
    "format": "prettier --write .",
    "lint": "eslint src"
  },
  "author": "Cansn0w",
  "license": "ISC",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "css-modules",
    "prerender",
    "pre-render",
    "prerendering",
    "minimal",
    "jest"
  ],
  "repository": "https://github.com/Cansn0w/react-typescript-webpack-prerender-template",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/loader-utils": "^2.0.6",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "http-proxy-middleware": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.0.3",
    "serve-static": "^1.15.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "yargs": "^17.7.2"
  }
}
